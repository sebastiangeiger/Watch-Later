{
  "name": "Watch Later",
  "description": "An improved version of Youtube Watch Later",
  "version": "0.1",
  "manifest_version": 2,
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "permissions": [
    "https://accounts.google.com/o/oauth2/token"
  ],
  "icons": { "16": "icon-16.png", "128": "icon-128.png" }
}
